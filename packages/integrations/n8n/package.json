{
  "name": "@cascadeflow/n8n-nodes-cascadeflow",
  "version": "1.1.0",
  "description": "n8n node for cascadeflow - Smart AI model cascading with 40-85% cost savings",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "cascadeflow",
    "ai",
    "llm",
    "cost-optimization"
  ],
  "license": "MIT",
  "homepage": "https://github.com/lemony-ai/cascadeflow",
  "author": {
    "name": "Lemony Inc.",
    "email": "hello@lemony.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemony-ai/cascadeflow.git",
    "directory": "packages/integrations/n8n"
  },
  "scripts": {
    "build": "node esbuild.config.mjs",
    "dev": "tsc --watch",
    "test": "vitest run",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" package.json --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "prepack": "pnpm -w --filter @cascadeflow/core build && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CascadeFlowApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/LmChatCascadeFlow/LmChatCascadeFlow.node.js",
      "dist/nodes/CascadeFlowAgent/CascadeFlowAgent.node.js"
    ]
  },
  "devDependencies": {
    "@cascadeflow/core": "^1.0.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "glob": "^11.0.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.7.1",
    "typescript": "^5.1.6",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@langchain/core": "^1.1.0"
  }
}
