{
  "models": {
    "providers": {
      "cascadeflow": {
        "baseUrl": "http://YOUR_CASCADEFLOW_HOST:8084/v1",
        "apiKey": "local",
        "api": "openai-completions",
        "models": [
          {
            "id": "cascadeflow",
            "name": "CascadeFlow",
            "reasoning": false,
            "input": ["text"],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "models": {
        "cascadeflow/cascadeflow": {
          "alias": "cascade"
        }
      }
    }
  }
}
